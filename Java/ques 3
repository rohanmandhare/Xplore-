import java.util.Scanner;
public class Main {
public static void main(String[] args) {
Scanner cs=new Scanner(System.in);
String str1;
str1=cs.nextLine();
str1=str1.toLowerCase();
for (int i=0 ; i<str1.length() ; i++){
    char ch = str1.charAt(i);
    if (ch=='a' || ch=='i' || ch =='e' || ch=='o' || ch=='u'){
        continue;
    }
    else{
        System.out.print(ch);
    }
}
    
}
    
}
